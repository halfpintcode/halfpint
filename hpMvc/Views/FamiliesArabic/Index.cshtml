<script>
    $(document).ready(function(){
        $('.bxslider').bxSlider({
            mode: 'fade',
            auto: true,
            autoControls: true,
            captions: true,
            autoHover: true,
            pause: 4000

        });
        $(".cb").colorbox({rel: 'gal1'});
        $(".checksScreen").colorbox({rel: 'checksGal', transition: "fade"});
        $('#lanSelector li').hover(function(){
            $(this).children("ul").slideDown("fast");
        },
				function(){
				    $(this).children("ul").slideUp("fast");
				}
		);
        $('a.sideLink').click(function(e){
            e.preventDefault();
            var oldSelect = $("#active a").attr('id') + "Info";
            $("#" + oldSelect).fadeOut();
            $("#" + this.id + "Info").fadeIn();
            $("#active").removeAttr('id');
            $("#" + this.id).parent().attr('id', 'active');
        });
    });
</script>