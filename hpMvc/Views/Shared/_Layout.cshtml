<html xmlns="http://www.w3.org/1999/xhtml">
<head>
	<meta name="keywords" content="" />
	<meta name="description" content="" />
	<meta http-equiv="content-type" content="text/html; charset=utf-8" />
	<title>@ViewBag.Title</title>
	<link href="@Url.Content("~/Content/Earthlingtwo/Style.css")" rel="stylesheet" type="text/css" media="screen" />
	<link href="@Url.Content("~/Content/themes/base/jquery.ui.all.css")" rel="stylesheet" type="text/css" />        	
	@if (IsSectionDefined("AdditionalLinkHead")) { 
				@RenderSection("AdditionalLinkHead", false)
	}
	<script src="//ajax.googleapis.com/ajax/libs/jquery/1.8.1/jquery.min.js" type="text/javascript"></script>
	<script src="//ajax.googleapis.com/ajax/libs/jqueryui/1.8.23/jquery-ui.min.js" type="text/javascript"></script>

	@*<script src="@Url.Content("~/Scripts/modernizr-1.7.min.js")" type="text/javascript"></script>*@	
	<script src="@Url.Content("~/Scripts/Layout.js")" type="text/javascript"></script>
	
	
	@if (IsSectionDefined("AdditionalScriptHead"))
	{ 
				@RenderSection("AdditionalScriptHead", false)
	}
</head>
<body>
<div id="spinner"> 
	please wait... 
</div> 
<div id="wrapper">
	<div id="header">
		<div id="logon">
			@Html.Partial("_LogOnPartial")
		</div> 
	    <div id="contactus">
	        <a href="mailto:CCC@Halfpintstudy.org?Subject=Contacting you">Contact us</a>
	    </div>

	</div>
	<!-- end #header -->
	
	<div id="menu">
		
		@if (IsSectionDefined("Menu"))
		{ 
			@RenderSection("Menu")
		}
		else
		{ 
			<ul id="ulMainMenu">
				<li>@Html.ActionLink("Home", "Index", "Home")</li>
				<li>@Html.ActionLink("Families", "Index", "Families")</li>
                <li>@Html.ActionLink("Staff", "Index", "Staff")</li>								
				<li id="dccLink">@Html.ActionLink("DCC", "Index", "Dcc")</li>
                <li id="coordLink">@Html.ActionLink("Coordinator", "Index", "Coordinator")</li>
                <li id="adminLink">@Html.ActionLink("Admin", "Index", "Admin")</li>
			</ul>
		}        
	</div>
	<!-- end #menu -->
	
	@*<div id="barMnu">
		@if (IsSectionDefined("BarMenu")) { 
			@RenderSection("BarMenu",false)
		}				
	</div>*@
	<div id="page">
		
		<div id="content">
			<div id="sidebar">
				@if (IsSectionDefined("Sidebar"))
	   { 
					@RenderSection("Sidebar", false)
	   }
			</div>
			<!-- end #sidebar -->

			@RenderBody()
			<div style="clear: both;">&nbsp;</div>

		</div>
		<!-- end #content -->
		
		<div style="clear: both;">&nbsp;</div>
	</div>
	<!-- end #page -->
</div> <!-- end #wrapper -->
<div id="footer-content">
	<img id="imgNih" alt="ct" src="@Url.Content("~/Content/Images/nih.png")" />
	<img id="imgct" alt="ct" src="@Url.Content("~/Content/Images/clinicaltrials.gif")" />
	<img id="imgnhlbi" alt="nhlbi" src="@Url.Content("~/Content/Images/nhli.gif")" />
</div>
<div id="footer">
	
	<p> </p>
</div>
<!-- end #footer -->
</body>
</html>
