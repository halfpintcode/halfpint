@{
    ViewBag.Title = "Initialize Subject";
    Layout = "~/Views/Shared/_Layout.cshtml";
}
<div id="downloadDialog">
    <p>
        Please wait... This download could take a few minutes.
    </p>
</div>
<div id="startDownloadDialog" class="divBtnDownload">
    <p class="textLeft">
        CHECKS is ready for download, click the begin download button to download and save
        CHECKS.
    </p>
    <p>
        <strong>Make sure you save CHECKS to the C:\\Halfpint directory!</strong>
    </p>
    <br />
    <br />
    <button id="btnDownload">
        Begin CHECKS Download</button>
    <br />
    <br />
    <p class="textLeft">
        This download could take up to two minutes. Do not move off this page until the
        download is complete. If however, this download doesn't start after two minutes
        then click on the alternate download link below.
    </p>
</div>
<div id="divLogin">
    <h3>
        Enter the study id and password for initializing this subject</h3>
    <br />
    <label class="lblLogin">
        Study ID:</label>
    <input type="text" name="studyID" id="studyID" value="" />
    <br />
    <label class="lblLogin">
        Password:</label>
    <input type="text" name="password" id="password" value="" />
    <br />
    <input type="button" id="btnLogin" value="Submit" />
</div>
<div id="divMain" class="hideStart">
    <div id="mainTitle">
        Initialize Study ID
    </div>
    <div id="divSideBar">
        <ul>
            <li><a href="#" id="lnkInfo">Information</a></li>
            <li><a href="#" id="lnkInstruc">Instructions</a></li>
            <li id="liCafpint"><a href="#" id="lnkCafpint">CAF-PINT</a></li>
            <li id="liSensor"><a href="#" id="lnkSensor">Start Medtronic Sensor</a></li>
            @*<li id="liSensorMedtronic"><a href="#" id="lnkSensorMedtronic">Start Medtronic Sensor</a></li>*@
            @*<li id="liSensorDexcom"><a href="#" id="lnkSensorDexcom">Start Dexcom Sensor</a></li>*@
            <li><a href="#" id="lnkParams">Order Insulin</a></li>
            <li id="liVamp"><a href="#" id="lnkVamp">Place Vamp Jr.</a></li>
            <li><a href="#" id="lnkCalGluc">QC Glucose Meter</a></li>
            <li><a href="#" id="lnkRandomize">Start CHECKS</a></li>
        </ul>
    </div>
    <form action="" method="post">
    <div id="div1Col">
        <hr />
        <input type="hidden" value='@ViewBag.SensorType' name="sensorType" id="sensorType"/>
        <input type="hidden" value='@ViewBag.UseCafpint' name="cafPint" id="cafPint"/>
        <input type="hidden" value='@ViewBag.UseVampjr' name="vampjr" id="vampjr"/>
        <input type="hidden" name="randDate" id="randDate" />
        <input type="hidden" name="randTime" id="randTime" />
        <div id="divInfo" class="mod">
            <h3>
                Information Page</h3>
            <hr />
            <p>
                This webpage will take you through a series of steps so that you can start a new
                subject on the HALF-PINT study.
            </p>
            <p>
                First confirm the following:
            </p>
            <div class="square">
                <ul>
                    <li>Your patient has a 2nd confirmed BG over 150</li>
                    <li>A study cart is available. Each ICU has 3 study carts. If all 3 carts are in use
                        contact your study coordinator</li>
                    <li>Inspect study cart to determine you have all the neccessary supplies <strong>prior</strong>
                        to randomization.
                        <ul>
                            <li class="sensorInfo">Sensor Supplies
                                <ul>
                                    <li>2 unopened sensor packages. Check expiration date.</li>
                                    <li>Transmitter in charger</li>
                                    <li>Monitor</li>
                                    <li>Inserter</li>
                                </ul>
                            </li>
                            <li>VAMP supplies
                                <ul>
                                    <li>2 VAMP devices</li>
                                    <li>Adequate sampling cannulas</li>
                                </ul>
                            </li>
                            <li>Study Glucose Meter (StatStrip)
                                <ul>
                                    <li>Meter</li>
                                    <li>Dock with spare battery</li>
                                    <li>A sufficient supply of test strips</li>
                                    <li>High QC vial</li>
                                    <li>Low QC vial</li>
                                </ul>
                            </li>
                        </ul>
                    </li>
                </ul>
            </div>
            <br />
            <input type="button" id="btnInfoNext" class="nextButton" value="I am ready to proceed" />
        </div>
        <div id="divInstruc" class="mod">
            <h3>
                Instructions Page</h3>
            <hr />
            <p>
                The next steps will include the following:
            </p>
            <div class="square">
                <ul>
                    <li class="sensorInfo">Sensor insertion by a trained staff member </li>
                    <li>Order insulin infusion </li>
                    <li>Placement of VAMP device </li>
                    <li>QC of StatStrip monitor </li>
                    <li>Start CHECKS
                        <ul>
                            <li>Refer to bedside nursing binder for steps to open and start CHECKS </li>
                            <li>CHECKS will assign blood glucose range </li>
                        </ul>
                    </li>
                </ul>
            </div>
            <br />
            <p>
                You can follow these steps in any order except starting the CHECKS algorithm, which
                is always the last step. Use the light blue menu on the left to skip from step to
                step. You may want to have the study bedside instruction binder nearby as you will
                need to refer to it during the final step of saving and opening the CHECKS file.
            </p>
            <p>
                If you have any questions during this process, the contact information for the study
                team is also in the study binder. When you are ready to proceed, click Next
            </p>
            <br />
            <input type="button" id="btnInstrucNext" class="nextButton" value="Next" />
        </div>
        <div id="divCafpint" class="mod">
            <h3>
                CAF-PINT</h3>
            <hr />
            <span>Did this subject provide consent for the CAF-PINT ancillary study?</span><br />
            <input type="radio" name="cafpintYesNo" value="yes" />Yes
            <input type="radio" name="cafpintYesNo" value="no" />No<br />
            <br />
            <div id="divCafpintYes">
                <span>Was this subject’s most recent INR > 3?</span><br />
                <input type="radio" name="cafpintYes" value="yes" />Yes
                <input type="radio" name="cafpintYes" value="no" />No
                <input type="radio" name="cafpintYes" value="no inr" />No INR documented
                <br />
                <br />
                <div id="divInr3Yes">
                    <span>This subject is not eligible for CAF-PINT due to an INR >3. Press 'Complete' to
                        continue to the next step.</span>
                </div>
                <div id="divInr3No">
                    <span>Enter subject’s CAF-PINT ID: </span>
                    <input type="text" id="cafPintId" name="cafPintId" />
                    <br />
                    <span>Remember to collect the study Day 0 (baseline) blood specimens for CAF-PINT prior
                        to starting insulin.</span>
                </div>
            </div>
            <br />
            <input type="button" id="btnCafpintNext" class="nextButton" value="Complete" />
        </div>
        <div id="divSensor" class="mod">
            <h3 id="h3SensorTitle">
                Start Medtronic Sensor</h3>
            <br />
            Enter the following sensor information (videos are available for review)
            <hr />
            <div id="divInnerSensor">
                <br />
                <p>
                    <label for="MonitorDate" id="lblSensorDate">
                        Monitor Date</label>
                    <input type="text" id="MonitorDate" name="MonitorDate" size="25" />
                </p>
                <p>
                    <label for="MonitorTime" id="lblSensorTime">
                        Monitor Time</label>
                    <input type="text" id="MonitorTime" name="MonitorTime" size="25" />
                </p>
                <p>
                    <label for="MonitorID" id="lblSensorId">
                        Monitor ID</label>
                    <input type="text" id="MonitorID" name="MonitorID" size="25" />
                </p>
                <p>
                    <label for="TransmitterID" id="lblSensorTransmitterId">
                        Transmitter ID</label>
                    <input type="text" id="TransmitterID" name="TransmitterID" size="25" />
                </p>
                <p>
                    <label for="SensorLot">
                        Sensor Lot</label>
                    <input type="text" id="SensorLot" name="SensorLot" size="25" />
                </p>
                <p>
                    <label for="ExpirationDate">
                        Sensor Expiration Date</label>
                    <input type="text" id="ExpirationDate" name="ExpirationDate" size="25" />
                </p>
                <p>
                    <label for="InserterFirstName">
                        Inserter First Name</label>
                    <input type="text" id="InserterFirstName" name="InserterFirstName" size="25" />
                </p>
                <p>
                    <label for="InserterLastName">
                        Inserter Last Name</label>
                    <input type="text" id="InserterLastName" name="InserterLastName" size="25" />
                </p>
                <p>
                    <label for="SensorLocation">
                        Sensor Location</label>
                    @Html.DropDownList("SensorLocations")
                </p>
                <br />
                <input type="button" id="btnSensorNext" class="nextButton" value="Complete" />
            </div>
            <div id="sensorImage">
                <img id="imgSensor" src='@Url.Content("~/Content/images/sensorMed.bmp")' alt="image" />
            </div>
            <div id="divMedtronicVids">
                <br />
                <br />
                <iframe src="//player.vimeo.com/video/36000592?title=0&amp;byline=0&amp;portrait=0"
                    width="533" height="300" frameborder="0" webkitallowfullscreen mozallowfullscreen
                    allowfullscreen></iframe>
                <p>
                    Guardian Monitor
                </p>
            </div>
            <div id="divDexcomVids">
                <br />
                <br />
                <iframe src="//player.vimeo.com/video/139509674" width="500" height="375" 
                        frameborder="0" webkitallowfullscreen mozallowfullscreen allowfullscreen></iframe>
                <p>
                    Dexcom G4 Sensor Video
                </p>
                <iframe src="//player.vimeo.com/video/139509331" width="500" height="375" 
                        frameborder="0" webkitallowfullscreen mozallowfullscreen allowfullscreen></iframe>
                <p>
                    Dexcom G4 Receiver Video
                </p>
            </div>
        </div>
