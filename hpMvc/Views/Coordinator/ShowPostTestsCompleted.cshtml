@model IEnumerable<hpMvc.Models.PostTestPersonTestsCompleted>

@{
    ViewBag.Title = "ShowPostTestsCompleted";
    Layout = "~/Views/Shared/_Layout.cshtml";
}
    
<h2>Post Tests Completed</h2>

<table id="tbl1">
    <tr>
        <th>
            Name
        </th>
        <th>Test &emsp;&emsp;&emsp;&emsp;&emsp;&emsp;&emsp;&emsp; Date Completed</th>
        
    </tr>


@foreach (var item in Model) {
    <tr>
        <td>
            @Html.DisplayFor(modelItem => item.Name)
        </td>
        <td>
            
                @foreach (var t in item.PostTestsCompleted) { 
                    
                    <label class="tName">@t.Name</label>
                    <label class="tDate">@t.sDateCompleted</label>                     
                    <br />
                }
            
        </td>
    </tr>
}

</table>

<link href="@Url.Content("~/Content/ShowPostTests.css")" rel="stylesheet" type="text/css" />
@*<script src="@Url.Content("~/Scripts/UserManagement.js")" type="text/javascript"></script>*@
