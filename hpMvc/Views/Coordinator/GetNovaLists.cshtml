@{
    ViewBag.Title = "Get Operators List";
    Layout = "~/Views/Shared/_Layout.cshtml";
}

<h2>Get Operators List</h2>
<br/>

<p>Files are in order by date with the lastest one first.</p>
<div>
    <input id="siteCode" name="siteCode" type="hidden" value='@ViewBag.SiteCode' />
    <span>Select a file: </span>&nbsp; @Html.DropDownList("Files") &nbsp;&nbsp; <input type="button" id="btnDownload" value="Download" />
    <br /><br />
            
</div>

<script src="@Url.Content("~/Scripts/GetNovaList.js")" type="text/javascript"></script>